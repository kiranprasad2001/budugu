{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "sourceMap": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "src/service-worker.ts"]
}